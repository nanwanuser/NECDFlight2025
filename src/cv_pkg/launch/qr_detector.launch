<launch>
  <!-- 加载航点参数 -->
  <rosparam command="load" file="$(find cv_pkg)/config/waypoints.yaml"/>

  <!-- 启动二维码检测节点 -->
  <node name="qr_logistics_detector" 
        pkg="cv_pkg" 
        type="qr_logistics_detector" 
        output="screen">
    <param name="camera_device" value="/dev/video2"/>  <!-- 修改为你的摄像头设备 -->
    <param name="show_image" value="true"/>
  </node>
</launch>